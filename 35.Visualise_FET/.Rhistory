#Visualise resuls from Popoolation2 Fishers Exact Test
library("reshape2")
library("ggplot2")
##someone please write a more generic way of doing this with variable population numbers!
## Create FET names
x        <- t(outer(1:6, 2:7, paste, sep="."))
FETnames <- paste("FET_", x[!upper.tri(x)], sep="")
my_col_names=c("reference_contig","position","N_SNPs","fraction","avg_minimum", FETnames)
my_col_names.old=c("reference_contig","position","N_SNPs","fraction","avg_minimum","FET_1.2","FET_1.3","FET_1.4","FET_1.5","FET_1.6","FET_1.7","FET_2.3","FET_2.4","FET_2.5","FET_2.6","FET_2.7","FET_3.4","FET_3.5","FET_3.6","FET_3.7","FET_4.5","FET_4.6","FET_4.7","FET_5.6","FET_5.7","FET_6.7")
## Sanity check:
identical(my_col_names, my_col_names.old)
measure_vars <- my_col_names[6:18]
id_vars      <- my_col_names[1:2]
identical(my_col_names, my_col_names.old)
